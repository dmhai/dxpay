@{
    ViewBag.Title = "母版页";
    string UserRegister = (string)ViewBag.UserRegister;
}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>盾行云计费-支持微信_支付宝_银联支付接口等的聚合支付平台</title>
    <meta name="author" content="聚合支付平台" />
    <META NAME="Subject" CONTENT="盾行聚合支付平台提供,一站式聚合支付  SDK, API  接口,移动支付更便捷!">
    <meta name="Copyright" content="Copyright (c) 2016 盾行云计费 | V2" />
    <meta name="description" content="盾行聚合支付平台 -凭借前沿支付理念和雄厚研发实力,作为第四方支付聚合平台,聚合微信,支付宝等众多第三方支付接口,为商户提供聚合支付 SDK, API  接口,带来更流畅的支付体验!" />
    <meta name="keywords" content="聚合支付,聚合支付平台,支付接口,支付API,支付宝接口,微信支付接口,银联支付接口,支付接入,app支付,游戏支付,扫码支付" />
    <link href="~/css/base.css" rel="stylesheet" type="text/css">
    <link href="~/css/main.css" rel="stylesheet" type="text/css">
    <link href="~/css/login.css" rel="stylesheet" />
    <script src="~/js/jquery-1.8.1.min.js"></script>

    <script src="~/js/plugin/layer/layer.js"></script>
    <script>
        function whatBrowser() {
            var domtwidth = document.body.clientWidth;
            if ((window.navigator.userAgent.indexOf('compatible') != -1) && (domtwidth < 1400)) {
                $('#main').css('height', '420px');
                $('.login-title').css('font-size', '30px');
                $('.login-subTitle').css('font-size', '20px');
                $('.idx-tit .tit').css('font-size', '30px');
                $('.idx-tit .subtit').css('font-size', '20px');
                $('.idx-tit').css('height', '100px');
                $('.idx-cnt').css('margin-top', '-240px');
                $('.idx-bank-lst p').css({ 'height': '110px', 'line-height': '110px', 'width': '24%' });
                $('.idx-bank-slider').css('height', '260px');
                $('.cslt-frm .txtipt, .cslt-frm .slec').css('height', '22px');
                $('.cslt-frm .error').css('height', '18px');
                $('.cslt-frm .traipt').css('height', '60px');
            }
        }
    </script>
</head>
<body class="index-body" onLoad="whatBrowser()">
    <div class="bodys" style="position:absolute; z-index:998; width:100%; height:100%; display:none;"></div>
    <header class="login-menu clearfix">
        <div class="dlogo fl clearfix">
            <a href="http://www.dunxingpay.com" class="logo fl" title="聚合支付" alt="聚合支付"></a>
            <span class="pcnt fl">极速、便捷、可靠的第三方支付聚合平台</span>
        </div>
        <nav class="nav fr">
            <ul class="nav-lst clearfix">
                <li class="on">
                    <a href="http://www.dunxingpay.com" title="聚合支付">首页</a>
                </li>
                <li>
                    <a href="/index/News" title="新闻资讯">新闻资讯</a>
                    <div class="nav-dcnt">
                        <i class="icon-sj"></i>
                        <dl class="nav-smart about-smart">
                            <dd>
                                <a href="/index/News.html" title="行业新闻">行业新闻</a>
                            </dd>
                            <dd class="active">
                                <a href="/index/zixun.html" title="公司资讯">公司资讯</a>
                            </dd>
                        </dl>
                    </div>
                </li>
                <li>
                    <a href="/index/product" title="产品支付">产品支付</a>
                    <div class="nav-dcnt">
                        <i class="icon-sj"></i>
                        <dl class="nav-smart product-smart">
                            <dd>
                                <a href="/index/product.html#0" title="微信支付">微信支付</a>
                            </dd>
                            <dd class="active">
                                <a href="/index/product.html#1" title="支付宝支付">支付宝支付</a>
                            </dd>
                            <dd>
                                <a href="/index/product.html#2" title="银联支付">银联支付</a>
                            </dd>
                            <dd>
                                <a href="/index/product.html#3" title="其他支付">其他支付</a>
                            </dd>
                        </dl>
                    </div>
                </li>

                <li>
                    <a href="/index/about" title="关于我们">关于我们</a>
                    <div class="nav-dcnt">
                        <i class="icon-sj"></i>
                        <dl class="nav-smart about-smart">
                            <dd>
                                <a href="/index/about.html#0" title="盾行简介">盾行简介</a>
                            </dd>
                            @*<dd class="active">
                                    <a href="/index/about.html#1" title="公司资讯">公司资讯</a>
                                </dd>*@
                            <dd>
                                <a href="/index/about.html#1" title="联系我们">联系我们</a>
                            </dd>
                        </dl>
                    </div>
                </li>

            </ul>
            <a href="@UserRegister" class="btn-bor" title="开发者注册">开发者注册</a>
        </nav>
    </header>
    @RenderBody()
    <aside class="idx-csult-fix" id="idx-csult-fix">
        <a href="javascript:void(0)" class="btn idx-cslt" title="商务咨询">商务咨询</a>
        <section class="idx-consult">
            <div class="cslt-cnt">
                <p class="tit">
                    商务咨询
                    <i class="icon-close" title="关闭"></i>
                </p>

                <form>
                    <ul class="cslt-frm">
                        <li>
                            <div class="name">姓名</div>
                            <div class="ipt">
                                <input type="text" id="Name" class="txtipt" placeholder="请输入姓名" onblur="yzname()" />
                            </div>
                            <div id="yzname" class="error"></div>
                        </li>
                        <li>
                            <div class="name">手机</div>
                            <div class="ipt">
                                <input type="text" id="MobilePhone" class="txtipt" placeholder="请输入手机" onblur="yzmobilePhone()" />
                            </div>
                            <div id="yzmobilePhone" class="error"></div>
                        </li>
                        <li>
                            <div class="name">QQ</div>
                            <div class="ipt">
                                <input type="text" id="QQ" class="txtipt" placeholder="请输入QQ" onblur="yzQQ()" />
                            </div>
                            <div id="YZQQ" class="error"></div>
                        </li>
                        <li>
                            <div class="name">邮箱</div>
                            <div class="ipt">
                                <input type="text" id="EmailAddress" class="txtipt" placeholder="请输入邮箱" />
                            </div>
                            <div id="yzemailAddress" class="error"></div>
                        </li>

                        <li>
                            <div class="name">产品日流水</div>
                            <div class="ipt">
                                <label for="Website_0"><input id="Website_0" type="radio" name="Website" value="0" checked="checked" />小于5万</label>
                                <label for="Website_1"><input id="Website_1" type="radio" name="Website" value="1" />大于5万到10万</label>
                                <label for="Website_1"><input id="Website_1" type="radio" name="Website" value="2" />15万以上</label>
                            </div>
                            <div class="error"></div>
                        </li>
                        <li>

                            <div class="name">产品需求</div>
                            <div class="ipt">
                                <textarea class="traipt" id="RequestContent" placeholder="请描述您的产品需要的支付需求"></textarea>
                            </div>
                            <div class="error"></div>
                        </li>
                        <li class="lbtn">
                            <a href="javascript:void(0);" id="send" class="btn-radr" title="商务咨询">发送</a>
                        </li>
                    </ul>
                </form>
                <div class="clr">
                    @*<p class="dtxt">关注<a href="#" class="lk">微信公众号</a>获取即时资讯</p>*@
                </div>

            </div>
        </section>
    </aside>
    @*<div class="layer-fixed" style="display:;"></div>*@
    <div class="wp-layer" id="wp-layer" style="display:none">
        <div class="wp-layer-cont">
            <i class="icon-close-lyr icon-close-btn"></i>
            <header class="tit">提示</header>
            <p class="ifo"><i class="icon-cricl"></i> 您的信息我们已经收到，稍后我们会联系您！</p>
            <a href="javascript:void(0);" class="btn btn-lyr icon-close-btn" title="成功提示">确定</a>
        </div>
    </div>

    <div id="floatTools" class="rides-cs">
        <div class="floatL">
            <a id="aFloatTools_Show" class="btnOpen" title="查看在线客服" href="javascript:void(0);">展开</a>
        </div>
        <div id="divFloatToolsView" class="floatR" style="display:none;">
            <div class="cn">
                <ul>
                    <li><span>客服1</span> <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&uin=3526801903&site=qq&menu=yes"><img border="0" src="~/images/online.png" alt="点击这里给我发消息" title="点击这里给我发消息" /></a> </li>
                    <li><span>客服2</span> <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&uin=1914033268&site=qq&menu=yes"><img border="0" src="~/images/online.png" alt="点击这里给我发消息" title="点击这里给我发消息" /></a> </li>
                    @*<li><span>客服2</span> <a target="_blank" href="http://wpa.qq.com/msgrd?v=3&uin=3370314881&site=qq&menu=yes"><img border="0" src="~/images/online.png" alt="点击这里给我发消息" title="点击这里给我发消息" /></a> </li>*@
                    <li><span>+86-0755-26910992</span></li>
                    <li>

                </ul>
            </div>
        </div>
    </div>

    <script src="~/js/base.js"></script>
    <script src="~/js/JValidator.js"></script>
    <script src="~/js/CooperationApplication.js"></script>
    <script src="~/js/jquery.SuperSlide.2.1.1.js"></script>
    <script src="~/asste/jquery.countup/jquery.waypoints.min.js"></script>
    <script src="~/asste/jquery.countup/jquery.countup.js"></script>
    <script src="~/asste/jquery.fullPage/jquery.fullPage.min.js"></script>
    <script src="~/asste/PicCarousel.js"></script>
    <script>
        $(function () {
            var idx = 0;
            $("#aFloatTools_Show").click(function () {
                if (idx == 0) {
                    $('#divFloatToolsView').animate({ width: 'show', opacity: 'show' }, 100);
                    idx = 1;
                } else {
                    $('#divFloatToolsView').animate({ width: 'hide', opacity: 'hide' }, 100);
                    idx = 0;
                }
            });
        });
    </script>
</body>
</html>
