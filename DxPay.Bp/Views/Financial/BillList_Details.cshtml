@{
    ViewBag.Title = "详情";

    var colist = ViewBag.colist as List<JMP.MDL.CoSettlementDeveloperOverview>;
}
@{
    <table border="0" cellpadding="0" cellspacing="0" style="width: 100%;">
        <thead>
            <tr>
                <th>用户名称</th>
                <th>流水金额</th>
                <th>服务费</th>
                <th>提成费率</th>
                <th>提成金额</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in colist)
            {
                if (item != null)
                {
                    <tr>
                        @if (ViewBag.appidType == 1)
                        {
                            <td>@item.DeveloperName</td>
                        }
                        else
                        {
                            <td>@item.DisplayName</td>
                        }
                        <td>@TOOL.DecimalDigit.DecimalPlacesToString(item.TotalAmount)</td>
                        <td>@TOOL.DecimalDigit.DecimalPlacesToString(item.ServiceFee)</td>
                        <td>@String.Format("{0:N2}", (item.BpPushMoneyRatio * 100)) % </td>
                        <td>@TOOL.DecimalDigit.DecimalPlacesToString(item.BpPushMoney)</td>
                    </tr>
                }
            }
        </tbody>
    </table>

}