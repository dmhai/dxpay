@model WEB.Models.AppUser.MerchantListViewModel
@{
    ViewBag.Title = "完成工单页面";
    Layout = "~/Views/UI/dialog.cshtml";

}
@JMP.TOOL.UserHelper.js("Workorder/Workorder.js")

<div class="main">
    <input type="hidden" id="id" value="@(ViewBag.id)" />
    <div class="clear10"></div>
    <div class="plate-form">
        <dl>
            <dt>工单进度：</dt>
            <dd>
                <input  type="radio" readonly="readonly" checked="checked" > 完成
            </dd>
        </dl>
        <dl>
            <dt>交流内容：</dt>
            <dd>
                <textarea name="handleResultDescription" type="text" id="handleResultDescription" > </textarea>              
            </dd>
        </dl>
    </div>
</div>
<div class="page-footer" style="width: 100%;">
    <div class="btn-listbtn">
        <input type="button" name="btnWorkorderWC" value="保存" id="btnWorkorderWC" class="btn btn-primary" />
        <input name="btnReturn" type="reset" onclick="btnCodesc()" value="取消" class="btn yellow" />
    </div>
    <div class="clear"></div>
</div>
<script type="text/javascript">
    //界面加载完成之后执行
    $(document).ready(function () {

    })
</script>