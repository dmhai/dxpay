@{
    ViewBag.Title = "设置费率";
    Layout = "~/Views/UI/dialog.cshtml";
}
@JMP.TOOL.UserHelper.js("Agent/userList.js")
<div class="main">

    <div class="clear10"></div>
    <div class="plate-form">
        <dl>
            <dt>费率：</dt>
            <dd>
                <div class="single-input normal">
                    <span class="ie7-input-bug-start"></span>
                    <input name="ServiceFeeRatioGradeName" type="text" id="ServiceFeeRatioGradeName" readonly="readonly" onclick="xzCoService()" onblur="CoWerviceYz()" value="" /><span class="ie7-input-bug-end"></span>
                    <input type="hidden" id="ServiceFeeRatioGradeId" name="ServiceFeeRatioGradeId" />
                </div>
                <div class="Validform_checktip" id="yzfl">* 请选择</div>
            </dd>
        </dl>
        <dl>
            <dt></dt>
            <dd>
                <input type="hidden" id="id" value="@ViewBag.id" />
            </dd>
        </dl>
    </div>
</div>
<div class="page-footer" style="width: 100%;">
    <div class="btn-listbtn">
        <input type="button" name="btnSave" value="保存" id="btnServiceFeeRatioGradeAdd" class="btn btn-primary" />
        <input name="btnReturn" type="reset" onclick="btnCodesc()" value="取消" class="btn yellow" />
    </div>
    <div class="clear"></div>
</div>
